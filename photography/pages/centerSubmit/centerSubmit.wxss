/* pages/centerSubmit/centerSubmit.wxss */
page {
  background: white;
}

.top {
display: flex;
justify-content: flex-start;
align-items: center;
flex-direction: row;
overflow: hidden;
margin-bottom: 20rpx;
margin-top: 20rpx;
}

.logo {
padding-left: 50rpx;
width: 120rpx;
height: 120rpx;
}

.logo-text {
margin-left: 40rpx;
background-color: #fcb01e;
color: #ffffff;
padding: 20rpx 40rpx 20rpx 40rpx;
}

.contact {
display: flex;
flex-direction: column;
justify-content: center;
align-content: flex-end;
align-items: flex-end;
font-size: 30rpx;
font-weight: normal;
padding-left: 200rpx;
}

.contact-t1 {
color: #b0b0b0;
}

.call {
width: 70rpx;
height: 70rpx;
margin-left: 5rpx;
padding-bottom: 10rpx;
vertical-align: middle;
}

.title-h1 {
font-size: 40rpx;
display: block;
font-weight: bold;
margin: 0 40rpx;
padding: 20rpx 0;
}

.text {
display: flex;
font-size: 32rpx;
font-weight: normal;
padding-left: 40rpx;
color: #b0b0b0;
}

.title-h2 {
font-size: 32rpx;
display: block;
font-weight: bold;
margin: 10rpx 40rpx 10rpx;
padding-left: 0rpx;
padding-top: 20rpx;
}

.tips {
display: flex;
font-size: 35rpx;
font-weight: bold;
padding-left: 40rpx;
padding-bottom: 20rpx;
}

.input-group {
padding-left: 40rpx;
padding-right: 40rpx;
}

.bottom {
display: flex;
z-index: 100;
justify-content:center;
align-items: center;
flex-direction: column;
overflow: hidden;
float: center;
margin-left: 20rpx;
width: 700rpx;
height: 400rpx;
background-image: url('data:image/png;base64,');
background-size: 100%;
background-repeat: no-repeat;
background-position: center;
border: 0rpx !important;
color: white !important;
font-size: 32rpx !important;
}

.info {
display: flex;
flex-direction: column;
justify-content: center;
align-content: center;
align-items: center;
margin: 40rpx;
}


.info-h1  {
font-size: 60rpx;
font-weight: bold;
padding-bottom: 20rpx;
color: #fcb01e;
}

.info-h2  {
font-size: 45rpx;
font-weight: bold;
}

.info-h3  {
font-size: 38rpx;
padding-bottom: 10rpx;
}

.info-text  {
font-size: 30rpx;
padding-bottom: 20rpx;
color: #b0b0b0;
text-align: center;
line-height: 200%;
}