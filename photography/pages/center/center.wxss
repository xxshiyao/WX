page {
    background: #f3f3f3;
}

.top {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-direction: row;
  overflow: hidden;
  margin-bottom: 20rpx;
}

.logo {
  padding-left: 50rpx;
  width: 120rpx;
  height: 120rpx;
}

.logo-text {
  margin-left: 40rpx;
  background-color: #fcb01e;
  color: #ffffff;
  padding: 20rpx 20rpx 20rpx 20rpx;
}

.contact {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-content: flex-end;
  align-items: flex-end;
  font-size: 30rpx;
  font-weight: normal;
  padding-left: 200rpx;
}

.contact-t1 {
  color: #b0b0b0;
}

.call {
  width: 70rpx;
  height: 70rpx;
  margin-left: 5rpx;
  padding-bottom: 10rpx;
  vertical-align: middle;
}

.title-h1 {
  border-top: 1px solid #efefef;
  font-size: 40rpx;
  display: block;
  font-weight: bold;
  margin: 0 40rpx;
  padding: 20rpx 0;
}

.text {
  display: flex;
  font-size: 35rpx;
  font-weight: normal;
  padding-left: 40rpx;
  color: #b0b0b0;
}

.title-h2 {
  border-top: 1px solid #efefef;
  font-size: 40rpx;
  display: block;
  font-weight: normal;
  margin: 0 40rpx;
  padding-left: 0rpx;
  padding-top: 20rpx;
}

.checked-parameter {
  color: #fcb01e !important;
  border: 1rpx;
  border-style: solid;
  border-color: #fcb01e;
}

.parameter-info {
  color: #b0b0b0;
  padding: 5rpx;
  font-size: 35rpx;
}

.parameter-row{
  padding-top: 10rpx;
  padding-bottom: 20rpx;
}

.parameter-col{
  padding-top: 20rpx;
  padding-bottom: 20rpx;  
}

.parameter-wrap {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-direction: row;
  flex-wrap: wrap;
  overflow: hidden;
  padding: 30rpx 50rpx 30rpx 0rpx;
}

.parameter-block {
  align-self: flex-end;
  padding: 30rpx 30rpx 30rpx 70rpx;
}

.parameter-wrap .title-h2  {
  border-top: 1px solid #efefef;
  font-size: 40rpx;
  display: block;
  font-weight: normal;
  margin: 0 40rpx;
  padding-left: 0rpx;
  padding-top: 20rpx;
  padding-right: 200rpx;
}

.tips {
  display: flex;
  font-size: 35rpx;
  font-weight: bold;
  padding-left: 40rpx;
  padding-bottom: 20rpx;
}

.input-group {
  padding-left: 40rpx;
  padding-right: 40rpx;
}

.submit {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: row;
  padding-top: 20rpx;
}

.info {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-content: center;
  align-items: center;
  margin: 40rpx;
}


.info-h1  {
  font-size: 60rpx;
  font-weight: bold;
  padding-bottom: 20rpx;
  color: #fcb01e;
}

.info-h2  {
  font-size: 45rpx;
  font-weight: bold;
}

.info-h3  {
  font-size: 38rpx;
  padding-bottom: 10rpx;
}

.info-text  {
  font-size: 30rpx;
  padding-bottom: 20rpx;
  color: #b0b0b0;
  text-align: center;
  line-height: 200%;
}