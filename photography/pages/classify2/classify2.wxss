page {
    background-color: white;
}

.body {
  display: flex;
  justify-content:center;
  flex-direction: column;
  align-items: center;
}

.class-l2 {
  display: flex;
  justify-content:center;
  align-items: center;
  flex-direction: column;
  overflow: hidden;
  float: center;
  width: 724rpx;
  box-shadow: 0px 2px 5px #e2e1e1;
  margin-bottom: 40rpx;
}

.class-l2 image {
  float: center;
  width: 724rpx;
  height: 407rpx;
}

.class-l2-bottom {
    background-image: url('data:image/png;base64,');
background-size: 25%;
background-repeat: no-repeat;
background-position: right bottom;
}

.description {
  display: flex;
  justify-content:center;
  align-items: center;
  flex-direction: column;
  overflow: hidden;
  float: center;
  width: 600rpx;
  margin-bottom: 20rpx;
}

.title {
  font-size: 32rpx;
  font-weight: bold;
  margin-top: 10rpx;
  margin-bottom: 10rpx;
}

.text {
  font-size: 25rpx;
  font-weight: normal;
  margin-bottom: 10rpx;
}

.more {
  font-size: 30rpx;
  font-weight: bold;
  color: #fcb01e;
  margin-bottom: 10rpx;
}