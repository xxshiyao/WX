/* pages/component/top-select/top-select.wxss */

.select {
  display: flex;
  justify-content:center;
  align-items: center;
  flex-direction: row;
  overflow: hidden;
  float: center;
  box-shadow: 0px 2px 5px #e2e2e2;
  margin-bottom: 40rpx;
}

.select view {
  display: flex;
  justify-content:center;
  align-items: center;
  flex-direction: column;
  overflow: hidden;
  float: center;
  font-size: 32rpx;
  width: 350rpx;
  margin: 20rpx 0rpx 20rpx 0rpx;
}

.top-select-split {
  color: #e2e2e2;
  font-size: 37rpx;
}

.popup-list {
  display: flex;
  justify-content: flex-start;
  align-items: left;
  flex-direction: column;
  overflow: hidden;
  margin-top: 20rpx;
  margin-left: 45rpx;
}

.popup-list-title {
  margin-top: 20rpx;
  font-size: 37rpx;
}

.parameter-row{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-direction: row;
  overflow: hidden;
  margin-top: 20rpx;
  padding-top: 20rpx;
}

.parameter-row-split {
  width: 50rpx;
}

.parameter-info {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: row;
  overflow: hidden;
  border: 1rpx;
  border-style: solid;
  border-color: #c4c4c4;
  height: 70rpx;
  width: 190rpx;
  color: #b0b0b0;
  font-size: 32rpx;
}

.checked-parameter {
  color: #fcb01e !important;
  border: 1rpx;
  border-style: solid;
  border-color: #fcb01e;
}

.popup-list-append {
  padding: 80rpx;
}

.submit {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-direction: column;
  overflow: hidden;
  margin-top: 20rpx;
  padding-top: 20rpx;
  position: fixed !important;
  z-index: 999;
  bottom: 10rpx;
  right: 25rpx;
}

.submit-background {
  position: fixed !important;
  z-index: 998;
  bottom: 0;
  right: 0;
  padding: 75rpx 250rpx;
  background-color: white;
}

.submit text {
  background-image: url('data:image/png;base64,');
  background-size: 100% 100%;
  background-repeat: no-repeat;
  background-position: center;
  padding: 30rpx 200rpx 40rpx 200rpx;
  color: white;
}