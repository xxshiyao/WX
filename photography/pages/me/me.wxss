/* pages/me/me.wxss */
.top-bg {
  position: absolute;
  width: 100%;
  height: 550rpx;
  z-index: 0;
}

.top-cover {
  position: absolute;
  width: 100%;
  height: 550rpx;
  z-index: 1;
  opacity: 0.70;
  background-color:black;
}

.top-logo {
  position: absolute;
  width: 40%;
  height: 30%;
  z-index: 2;
  top: 50rpx;
  left: 210rpx;
}

.card {
  position: absolute;
  width: 80%;
  height: 180rpx;
  z-index: 3;
  top: 520rpx;
  left: 80rpx;
  background-color: white;
  box-shadow: 0px 2px 5px #e2e1e1;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: row;
}


.card-text {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  font-size: 32rpx;
  font-weight: normal;
  white-space: nowrap;
  overflow: hidden;
  letter-spacing: 0.05rem;
  color: #999999;
  padding-left: 60rpx;
  padding-right: 60rpx;
}

.card-text-number {
  font-weight: bold;
  margin-top: 10rpx;
  color: #faa119;
  font-size: 35rpx;
}

.card-icon {
  width: 60rpx;
  height: 60rpx;
  margin-bottom: 20rpx;
}

.message {
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  font-size: 32rpx;
  font-weight: normal;
  white-space: nowrap;
  overflow: hidden;
  letter-spacing: 0.05rem;
  color: #999999;
  padding-top: 780rpx;
}

.message text {
  margin-bottom: 20rpx;
}

.message-title {
  color: black;
  font-size: 34rpx;
  font-weight: bold;
}

.message-button[plain] {
  display: flex;
  justify-content:center;
  align-items: center;
  flex-direction: column;
  overflow: hidden;
  float: center;
  margin-left: 20rpx;
  width: 700rpx;
  height: 400rpx;
  background-image: url('data:image/jpg;base64,');
  background-size: 100%;
  background-repeat: no-repeat;
  background-position: center;
  border: 0;
  color: white;
  font-size: 34rpx;
  position: absolute;
  top: 900rpx;
}