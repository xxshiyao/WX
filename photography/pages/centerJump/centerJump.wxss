/* pages/centerJump/centerJump.wxss */
.logo {
  display: flex;
  justify-content:center;
  align-items: center;
  flex-direction: column;
  overflow: hidden;
  float: center;
  margin-top: 200rpx;
}

.logo image {
  width: 400rpx;
  height: 400rpx;
}

.jump {
  display: flex;
  justify-content:center;
  align-items: center;
  flex-direction: column;
  overflow: hidden;
  float: center;
  margin-left: 150rpx;
  margin-right: 150rpx;
}

.bottom {
  display: flex;
  justify-content:center;
  align-items: center;
  flex-direction: column;
  overflow: hidden;
  float: center;
  margin-left: 20rpx;
  width: 700rpx;
  height: 400rpx;
  background-image: url('data:image/jpg;base64,');
  background-size: 100%;
  background-repeat: no-repeat;
  background-position: center;
}
.bottom text {
  color: white;
  font-size: 40rpx;
  font-weight: 400;
}
