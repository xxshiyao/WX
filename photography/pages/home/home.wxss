/* pages/hot/hot.wxss */

page {
  padding-bottom: 10rpx;
  /* margin-left: 10rpx; */
  margin-right: 10rpx;
}

swiper {
    height: 360rpx;
    margin-bottom: 30rpx;
}

swiper-item {
    padding: 0 10rpx;
}

swiper-item image {
    width: 100%;
    height: 360rpx;
}

.swiper-item {
    position: relative;
    overflow: hidden;
    height: 360rpx;
}

.topArea {
  display: flex;
  justify-content:center;
  align-items: center;
  flex-direction: column;
  overflow: hidden;
  float: center;
  top: 320rpx;
  left: 50rpx;
  position: absolute;
  background-image: url('data:image/png;base64,');
  background-repeat:no-repeat;
  background-size:100% 100%;
  -moz-background-size:100% 100%;
}

.topText1 {
  padding: 50rpx 0rpx 10rpx 0rpx;
  font-size: 40rpx;
  font-weight: normal;
  letter-spacing: 5px;
  display: block;
}

.topText2 {
  padding: 0rpx 50rpx 40rpx 50rpx;
  font-size: 25rpx;
  letter-spacing: 1px;
  display: block;
}

.homeVideo {
  display: flex;
  justify-content:center;
  align-items: center;
  flex-direction: column;
  overflow: hidden;
  float: center;
  margin-bottom: 20rpx;
  margin-top: 150rpx;
}
.shanpai {
  display: flex;
  justify-content:center;
  align-items: center;
  flex-direction: column;
  overflow: hidden;
  float: center;
}

.top-title .main-title {
    padding: 25rpx 40rpx;
    font-size: 70rpx;
    font-weight: bold;
    display: block;
}

.main-view {
    position: absolute;
    bottom: 20rpx;
    right: 40rpx;
    color: #fff;
    font-size: 28rpx;
}

.main-view image {
    width: 36rpx;
    height: 36rpx;
    display: inline-block;
    vertical-align: middle;
}

.list {
  display: flex;
  justify-content:center;
  align-items: center;
  flex-direction: column;
  overflow: hidden;
  margin-top: 20rpx;
  float: center;
}

.title {
  display: flex;
  justify-content:center;
  align-items: center;
  flex-direction: column;
  overflow: hidden;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  float: center;
}

.title text {
  font-size: 45rpx;
  font-weight: 500;
  display: block;
}

.class-l1 {
  display: flex;
  justify-content:center;
  align-items: center;
  flex-direction: column;
  overflow: hidden;
  float: center;
  box-shadow: 0px 2px 5px #e2e1e1;
  margin-bottom: 40rpx;
}

.class-l1 image {
  float: center;
  width: 700rpx;
  height: 407rpx;
}

.class-l1 text {
  font-size: 35rpx;
  display: block;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
}

.costomized {
  display: flex;
  justify-content:center;
  align-items: center;
  flex-direction: column;
  overflow: hidden;
  margin-bottom: 50rpx;
}

.costomized image {
  float: center;
  width: 700rpx;
  height: 198rpx;
}